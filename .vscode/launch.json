{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
            },
        },
        {
            "name": "Python Debugger: Prepare BCSS from TIGER",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/pathseg/preprocessing/bcss/prepare.py",
            "args": [
                "--raw-data-dir",
                "/mnt/nas7/data/Personal/Valentin/aws_downloads/tiger-training/wsirois/roi-level-annotations/tissue-bcss",
                "--output-dir",
                "${workspaceFolder}/data/fss/bcss",
            ],
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
            },
        },
        {
            "name": "Python Debugger: Prepare IGNITE",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/pathseg/preprocessing/ignite/prepare.py",
            "args": [
                "--raw-data-dir",
                "/mnt/nas7/data/Personal/Valentin/IGNITE",
                "--output-dir",
                "${workspaceFolder}/data/fss/ignite",
            ],
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
            },
        },
        {
            "name": "Python Debugger: Prepare ANORAK",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/pathseg/preprocessing/anorak/prepare.py",
            "args": [
                "--raw-data-dir",
                "/mnt/nas7/data/Personal/Valentin/ANORAK_not_resized",
                "--output-dir",
                "${workspaceFolder}/data/fss/anorak",
            ],
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
            },
        },
        {
            "name": "Python: main.py fit ANORAK Linear Semantic",
            "type": "debugpy",
            "request": "launch",
            "module": "pathseg",
            "args": [
                "fit",
                "-c",
                "configs/anorak_linear_semantic.yaml",
                "--no_compile",
                // "--trainer.logger=false",
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "CUDA_VISIBLE_DEVICES": "1",
                "HYDRA_FULL_ERROR": "1"
            },
            "justMyCode": false
        },
        {
            "name": "Python: main.py fit IGNITE Linear Semantic",
            "type": "debugpy",
            "request": "launch",
            "module": "pathseg",
            "args": [
                "fit",
                "-c",
                "configs/ignite_linear_semantic.yaml",
                "--no_compile",
                "--trainer.logger=false",
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "CUDA_VISIBLE_DEVICES": "1",
                "HYDRA_FULL_ERROR": "1"
            },
            "justMyCode": false
        },
        {
            "name": "Python: main.py fit IGNITE Linear Mask2Former",
            "type": "debugpy",
            "request": "launch",
            "module": "pathseg",
            "args": [
                "fit",
                "-c",
                "configs/ignite_mask2former.yaml",
                "--no_compile",
                "--trainer.logger=false",
            ],
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}",
                "CUDA_VISIBLE_DEVICES": "1",
                "HYDRA_FULL_ERROR": "1"
            },
            "justMyCode": false
        },
    ]
}